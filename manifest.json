{

    "manifest_version": 3,
    "name": "Todoro",
    "version": "1.0.0",
  
    
    "action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
        },
    "description": "Todoro is a Pomodoro Timer & To-Do List.",
    "icons": {"128": "icon.png"},
    "background": {
        "service_worker": "background.js"
      },
      
    "permissions": [
        "storage"
    ]
    
}